# Flask 配置
SECRET_KEY=your-secret-key-change-this
FLASK_ENV=development

# 数据库配置
# DATABASE_URL=sqlite:///instance/database.db

# Google Gemini API 配置
GOOGLE_API_KEY=your-google-api-key
GOOGLE_API_BASE=https://generativelanguage.googleapis.com

# AI Provider 格式配置
# 可选值: "gemini" (Google GenAI SDK) 或 "openai" (OpenAI SDK)
AI_PROVIDER_FORMAT=gemini

# GenAI (Gemini) 格式专用配置
GENAI_TIMEOUT=300.0
GENAI_MAX_RETRIES=2

# OpenAI 格式专用配置（当 AI_PROVIDER_FORMAT=openai 时使用）
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_BASE=https://aihubmix.com/v1
OPENAI_TIMEOUT=300.0
OPENAI_MAX_RETRIES=2

# AI 模型配置
TEXT_MODEL=gemini-3-flash-preview
IMAGE_MODEL=gemini-3-pro-image-preview

# MinerU 文件解析服务配置
MINERU_TOKEN=your-mineru-token
MINERU_API_BASE=https://mineru.net

# 图片识别模型配置
IMAGE_CAPTION_MODEL=gemini-3-flash-preview

# 并发配置
MAX_DESCRIPTION_WORKERS=5
MAX_IMAGE_WORKERS=8

# 图片生成配置
DEFAULT_ASPECT_RATIO=16:9
DEFAULT_RESOLUTION=2K

# 日志配置
LOG_LEVEL=INFO

# CORS 配置
CORS_ORIGINS=http://localhost:3000

# 输出语言配置
# 可选值: 'zh' (中文), 'ja' (日本語), 'en' (English), 'auto' (自动)
OUTPUT_LANGUAGE=zh

# 火山引擎配置（用于 Inpainting 图像消除功能）
# 获取密钥: https://console.volcengine.com/iam/keymanage/
VOLCENGINE_ACCESS_KEY=your-volcengine-access-key
VOLCENGINE_SECRET_KEY=your-volcengine-secret-key
VOLCENGINE_INPAINTING_TIMEOUT=60
VOLCENGINE_INPAINTING_MAX_RETRIES=3
